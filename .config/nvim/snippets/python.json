{
  "Dump to json file": {
    "prefix": "wjson",
    "body": [
      "import json",
      "",
      "def write_to_json(data: dict, filename: str) -> None:",
      "    with open(filename, 'w') as file:",
      "        json.dump(data, file, indent=4)"
    ]
  },
  "Read data from json file": {
    "prefix": "rjson",
    "body": [
      "import json",
      "",
      "def read_from_json(filename: str) -> dict:",
      "    with open(filename, 'r') as file:",
      "        data = json.load(file)",
      "    return data"
    ]
  }
}
